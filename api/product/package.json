{
  "name": "product",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "jest --config ./jest.config.ts --passWithNoTests --runInBand",
    "lint": "eslint \"*/**/*.{js,jsx,ts,tsx}\"",
    "format": "prettier --write \"*/**/*.{js,jsx,ts,tsx}\"",
    "dev": "ts-node-dev ./src/index.ts",
    "migration:create": "typeorm-ts-node-commonjs migration:create src/migrations/initial.ts",
    "migration:generate": "typeorm-ts-node-commonjs migration:generate src/migrations/version -d src/data-source.ts",
    "migration:up": "typeorm-ts-node-commonjs migration:run -d src/data-source.ts",
    "migration:down": "typeorm-ts-node-commonjs migration:revert -d src/data-source.ts",
    "migration:show": "typeorm-ts-node-commonjs migration:show -d src/data-source.ts"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.15.0",
    "@types/cors": "^2.8.17",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.9.1",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "cookie": "^1.0.2",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.5",
    "ts-node-dev": "^2.0.0"
  },
  "description": "",
  "dependencies": {
    "@apollo/server": "^4.11.2",
    "argon2": "^0.41.1",
    "class-validator": "^0.14.1",
    "cookie": "^1.0.2",
    "csv-parse": "^5.6.0",
    "dotenv": "^16.4.5",
    "graphql": "^16.9.0",
    "graphql-scalars": "^1.23.0",
    "graphql-tag": "^2.12.6",
    "jest": "^29.7.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.13.1",
    "redis": "^4.7.0",
    "ts-node": "^10.9.2",
    "type-graphql": "^2.0.0-rc.2",
    "typeorm": "^0.3.20"
  }
}
